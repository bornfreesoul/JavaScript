<script type ="text/javascript">

//While loop example//
var counter = 0;
while(counter <= 10)
{
document.write("<br \>" + "The value of counter is: " + counter);
counter++;
} 

//Do-While loop//
var counter = 0;
do
{
document.write("<br \>" + "This is the value of do-while counter is: ", counter);
counter++;
}
while(counter <= 10)

</script>

